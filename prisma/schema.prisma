generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EditSheetCell {
  id Int @default(autoincrement()) @id
  authMode AuthMode
  oldValue String
  range Int
  source String
  triggerUid String
  userEmail String
  value String
}

enum AuthMode {
  NONE
  CUSTOM_FUNCTION
  LIMITED
  FULL
}
